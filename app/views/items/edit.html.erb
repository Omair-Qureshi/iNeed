
  <%= turbo_frame_tag "item_#{@item.id}" do %>
    <%= simple_form_for @item do |f| %>
      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.button :submit %>
    <% end %>
  <% end %>





<%= turbo_frame_tag "second_frame" do %>

<% if @item.photo.attached? %>
  <%= cl_image_tag(@item.photo.key, width: 150, height: 150) %>
  <% end %>
<% end %>
